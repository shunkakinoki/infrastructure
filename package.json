{
  "name": "journal",
  "author": "Shun Kakinoki",
  "license": "MIT",
  "version": "2.4.42",
  "homepage": "https://github.com/shunkakinoki/journal",
  "repository": {
    "type": "git",
    "url": "https://github.com/shunkakinoki/journal.git"
  },
  "scripts": {
    "dev": "vuepress dev src",
    "build": "vuepress build src",
    "test": "yarn run lint && yarn run release-test",
    "release": "semantic-release",
    "release-test": "semantic-release -d",
    "lint": "yarn eclint-check && yarn prettier-check",
    "fix": "yarn eclint-fix && yarn prettier-fix",
    "eclint-check": "eclint check $(git ls-files)",
    "eclint-fix": "eclint fix $(git ls-files)",
    "prettier-check": "prettier --check \"**/*.{js,jsx,json,ts,tsx,css,html,yml,yaml,scss,sass}\"",
    "prettier-fix": "prettier --write \"**/*.{js,jsx,json,ts,tsx,css,html,yml,yaml,scss,sass}\""
  },
  "dependencies": {
    "@vssue/vuepress-plugin-vssue": "^1.4.6",
    "@vuepress/plugin-active-header-links": "^1.5.1",
    "@vuepress/plugin-back-to-top": "^1.3.1",
    "@vuepress/plugin-google-analytics": "^1.5.1",
    "@vuepress/plugin-nprogress": "^1.5.1",
    "@vuepress/plugin-search": "^1.5.1",
    "vuepress": "^1.5.1",
    "vuepress-plugin-rss": "^2.0.0",
    "vuepress-plugin-sitemap": "^2.3.1",
    "vuepress-theme-yuu": "^2.3.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "commitizen": "^4.1.2",
    "eclint": "^2.8.1",
    "git-cz": "^4.6.2",
    "husky": "^4.2.5",
    "semantic-release": "^17.0.8"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git-cz --hook || true"
    }
  }
}
