{
  "name": "journal",
  "author": "Shun Kakinoki",
  "license": "MIT",
  "version": "2.1.2",
  "homepage": "https://github.com/shunkakinoki/journal",
  "repository": {
    "type": "git",
    "url": "https://github.com/shunkakinoki/journal.git"
  },
  "scripts": {
    "dev": "vuepress dev src",
    "build": "vuepress build src",
    "test": "yarn run lint && yarn run release-test",
    "release": "semantic-release",
    "release-test": "semantic-release -d",
    "lint": "yarn eclint-check",
    "fix": "yarn eclint-fix",
    "eclint-check": "eclint check $(git ls-files)",
    "eclint-fix": "eclint fix $(git ls-files)"
  },
  "dependencies": {
    "@vssue/vuepress-plugin-vssue": "^1.4.2",
    "@vuepress/plugin-active-header-links": "^1.3.1",
    "@vuepress/plugin-back-to-top": "^1.3.0",
    "@vuepress/plugin-google-analytics": "^1.3.1",
    "@vuepress/plugin-nprogress": "^1.3.1",
    "@vuepress/plugin-search": "^1.3.0",
    "vuepress": "^1.3.1",
    "vuepress-plugin-rss-support": "^1.0.2",
    "vuepress-theme-yuu": "^2.2.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/exec": "^5.0.0",
    "commitizen": "^4.0.3",
    "eclint": "^2.8.1",
    "git-cz": "^4.3.1",
    "husky": "^4.2.3",
    "semantic-release": "^17.0.4"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git-cz --hook || true"
    }
  }
}
